@model Fpm.MainUI.ViewModels.Upload.UploadIndexViewModel

@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    ViewBag.Title = "FPM - Upload data";
}

@section ExtraScripts
{
    @Html.Partial("_AngularScripts")
}

<app-upload id="app-upload-container"
            batch-template-url="@Model.BatchTemplateUrl"
            batch-last-updated="@Model.BatchLastUpdated"
            current-user="@Model.User.Id">
</app-upload>


@using Fpm.MainUI

@{
    ViewBag.Title = "FPM - Reports";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

@section ExtraScripts
{
    <script src="@(AppConfig.JsPath)angular.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)angular-route.js" type="text/javascript"></script>
    <script src="@(AppConfig.JsPath)jquery.form.js" type="text/javascript"></script>
}

@section AngularApp
{
    <script src="@(AppConfig.AngularAppPath)reports/reports-app.js" type="text/javascript"></script>
}

<div class="standardWidth">
    <h2 class="subheading">Reports</h2>
</div>
<div class="container" ng-app="reportsApp">
    <div ng-view></div>
</div>


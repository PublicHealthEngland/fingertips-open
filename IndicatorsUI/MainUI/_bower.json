{
  "name": "mainui",
  "homepage": "https://github.com/PublicHealthEngland/fingertips",
  "authors": [
    "Farrukh Ayub <farrukh.ayub@phe.gov.uk>"
  ],
  "description": "",
  "main": "",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "#1.12.4",
    "modernizr": "#v2.6.2",
    "js-cookie": "^2.1.2",
    "hogan.js": "hogan#^3.0.2",
    "google-maps-utility-library-v3-infobox": "^1.1.14",
    "tablesorter": "*",
    "underscore": "^1.8.3",
    "jquery.floatThead": "floatThead#^1.4.2",
    "html2canvas": "#0.5.0-alpha2",
    "chosen": "^1.8.5",
    "bootstrap": "bootstrap#v4.0.0",
    "tether": "^1.4.3",
    "components-font-awesome": "^4.7.0",
    "popper.js": "1.12.9"
  },
  "install": {
    "path": "js/vendor",
    "sources": {
      "popper.js": "bower_components/popper.js/dist/umd/popper.min.js",
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "chosen": [
        "bower_components/chosen/chosen.css",
        "bower_components/chosen/chosen.jquery.js",
        "bower_components/chosen/chosen-sprite.png",
        "bower_components/chosen/chosen-sprite@2x.png"
      ],
      "google-maps-utility-library-v3-infobox": "bower_components/google-maps-utility-library-v3-infobox/dist/infobox.min.js",
      "hogan.js": {
        "mapping": [
          {
            "bower_components/hogan.js/web/builds/3.0.2/hogan-3.0.2.min.js": "hogan.min.js"
          }
        ]
      },
      "html2canvas": [
        "bower_components/html2canvas/dist/html2canvas.min.js",
        "bower_components/html2canvas/dist/html2canvas.svg.min.js"
      ],
      "jquery.floatThead": "bower_components/jquery.floatThead/dist/jquery.floatThead.min.js",
      "bootstrap": {
        "mapping": [
          { "bower_components/bootstrap/dist/css/bootstrap.min.css": "../../../css/bootstrap/bootstrap.min.css" },
          { "bower_components/bootstrap/dist/js/bootstrap.min.js": "bootstrap.min.js" }
        ]
      },
      "underscore": [
        "bower_components/underscore/underscore-min.js"
      ],
      "modernizr": "bower_components/modernizr/modernizr.js",
      "tether": "bower_components/tether/dist/js/tether.min.js",
      "components-font-awesome": {
        "mapping": [
          { "bower_components/components-font-awesome/css/font-awesome.min.css": "../../../css/vendor/font-awesome/font-awesome.min.css" },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.woff": "../../../css/vendor/fonts/fontawesome-webfont.woff"
          },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.woff": "../../../fonts/fontawesome-webfont.woff"
          },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.woff2": "../../../css/vendor/fonts/fontawesome-webfont.woff2"
          },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.woff2": "../../../fonts/fontawesome-webfont.woff2"
          },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.ttf": "../../../css/vendor/fonts/fontawesome-webfont.ttf"
          },
          {
            "bower_components/components-font-awesome/fonts/fontawesome-webfont.ttf": "../../../fonts/fontawesome-webfont.ttf"
          }
        ]
      }
    }
  }
}
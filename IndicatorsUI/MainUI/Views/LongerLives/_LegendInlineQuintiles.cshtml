@using IndicatorsUI.DomainObjects
@model IndicatorsUI.MainUI.Models.PageModel
@if (Model.ProfileId != ProfileIds.SuicidePrevention)
{
    if (ViewBag.ShowQuintilesLegend)
    {
        // Quintiles
        <p id="high-to-low-legend">
            <span id="legend-header"></span>
            <span class="grade">
                low
                <img class="quintile-1" src="@(ViewBag.ImagesUrl)Mortality/grade-quintile-1.png" />
                <img src="@(ViewBag.ImagesUrl)Mortality/grade-quintile-2.png" />
                <img src="@(ViewBag.ImagesUrl)Mortality/grade-quintile-3.png" />
                <img src="@(ViewBag.ImagesUrl)Mortality/grade-quintile-4.png" />
                <img src="@(ViewBag.ImagesUrl)Mortality/grade-quintile-5.png" />high
            </span>
            <span class="grade">
                <img src="@(ViewBag.ImagesUrl)Mortality/no-data.png" alt="" />data unavailable
            </span>
        </p>
    }
}

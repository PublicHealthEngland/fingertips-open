@using IndicatorsUI.MainUI.Models
@model PageModel
@if (Model.HasAnyData)
{
    // Set target url
    string href = "/profile-group/" + Model.ProfileCollections.First().UrlKey +"/profile/" + ViewBag.ProfileUrlKey+ "/data";
    if (Model.DefaultFingertipsTabId != -1)
    {
        href += "#page/" + Model.DefaultFingertipsTabId;
    }

    @Html.Partial("~/Views/Shared/_StartButtonForHref.cshtml", href)
}

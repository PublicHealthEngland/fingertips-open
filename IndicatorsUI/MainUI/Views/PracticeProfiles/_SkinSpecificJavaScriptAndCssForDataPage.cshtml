@using System.Web.Optimization
@using Profiles.DataAccess
@using Profiles.DomainObjects

<link rel="Stylesheet" type="text/css" href="@(Model.CssPath)chosen/chosen.min.css" />
@Scripts.Render("~/bundles/js-practice-profiles")
@{
    // Set Practice profiles PDF URL
    var host = AppConfig.Instance.IsEnvironmentLive
        ? "http://fingertipsreports.phe.org.uk"
        : "https://testfingertipsreports.phe.org.uk";

    var pdfUrl = host + "/gpp/index.php";
}
<script type="text/javascript">//<![CDATA[
    populationGroupId = @GroupIds.PracticeProfiles_SupportingIndicators;
    earliestYear = 2010;
    latestYear = 2016;
    profileCollectionIdList = [];
    restrictSearchProfileId = @(Model.ProfileId);
    FT.url.pdf = '@(pdfUrl)';
//]]</script>

@using Profiles.MainUI.Models
@model PageModel
@if (Model.HasAnyData)
{
    if (Model.DefaultFingertipsTabId == -1)
    {
        <div class="start-button">
            <div class="btn">
                <a href="/profile/@ViewBag.ProfileUrlKey/data" title="START Go to the data">
                    START
                    <p>Go to the data</p>
                </a>
            </div>
        </div>
    }
    else
    {
        <div class="start-button">
            <div class="btn">
                <a href="/profile/@ViewBag.ProfileUrlKey/data#page/@Model.DefaultFingertipsTabId" title="START Go to the data">
                    START
                    <p>Go to the data</p>
                </a>
            </div>
        </div>
    }
}

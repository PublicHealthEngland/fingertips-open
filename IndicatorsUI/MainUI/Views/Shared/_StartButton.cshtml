@using Profiles.MainUI.Models
@model PageModel
@if (!Model.HasExclusiveSkin)
{
    if (Model.DefaultFingertipsTabId == -1)
    {
        <a id="startButton" href="/profile/@Model.ProfileUrlKey/data"></a>
    }
    else
    {
        <a id="startButton" href="/profile/@Model.ProfileUrlKey/data#page/@Model.DefaultFingertipsTabId"></a>
    }
}
